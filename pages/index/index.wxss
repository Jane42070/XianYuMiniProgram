swiper {
	display: flex;
	justify-content: center;
	align-items: center;
	width: 100%;
	height: 380rpx;
}
swiper-item {
	/* color: var(--themeColor); */
	display: flex;
	justify-content: center;
	align-items: center;
	padding: 0 30rpx 0;
}

swiper-item image {
	width: 690rpx;
	height: 350rpx;
	border-radius: 40rpx;
	box-shadow: 0 0 10rpx 2rpx rgba(0, 0, 0, 0.5);
}

.category {
	display: flex;
	justify-content: space-around;
	align-items: center;
	width: 650rpx;
	border-radius: 20rpx;
	margin: 30rpx 45rpx 0;
	padding: 15rpx 0;
	box-shadow: 0 0 10rpx 2rpx rgba(0, 0, 0, 0.5);
}

.category navigator {
	display: flex;
	align-items: center;
	height: 100rpx;
	flex-direction: column;
}

.category navigator text {
	font-size: 20rpx;
}

.category navigator image {
	width: 100%;
}

.index_floor {
	margin: 0 40rpx 0;
}

.index_floor .floor_group .floor_title image {
	width: 100%;
	height: 100%;
}

.index_floor .floor_group .floor_list navigator {
	width: 33.33%;
	float: left;
	overflow: hidden;
}

.index_floor .floor_group .floor_list navigator:nth-last-child(-n + 4) {
	height:  24.92711207vw;
	border-left: 10rpx solid #fff;
}

.index_floor .floor_group .floor_list navigator image {
	width: 100%;
	height: 100%;
}

.floor_title image{
	margin-top: 50rpx;
}

.floor_title {
	padding: 10rpx;
}

.floor_list navigator:nth-child(2),
.floor_list navigator:nth-child(3) {
	border-bottom: 10rpx solid #fff;
}